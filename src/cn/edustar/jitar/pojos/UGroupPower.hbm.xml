<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.edustar.jitar.pojos.UGroupPower" table="U_GroupPower">
        <id name="groupPowerId" type="java.lang.Integer">
            <column name="GroupPowerId" />
            <generator class="identity" />
        </id>
        <property name="groupId" type="java.lang.Integer">
            <column name="GroupId" />
        </property>
        <property name="uploadArticleNum" type="java.lang.Integer">
            <column name="UploadArticleNum" />
        </property>
        <property name="uploadResourceNum" type="java.lang.Integer">
            <column name="UploadResourceNum" />
        </property>
        <property name="uploadDiskNum" type="java.lang.Integer">
            <column name="UploadDiskNum" />
        </property>
        <property name="videoConference" type="java.lang.Integer">
            <column name="VideoConference" />
        </property>        
    </class>
</hibernate-mapping>
