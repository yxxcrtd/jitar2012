<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.edustar.push.MashupUser" table="MashupUser">
        <id name="mashupUserId" type="java.lang.Integer">
            <column name="MashupUserId" />
            <generator class="identity" />
        </id>
        <property name="mashupUserGuid" type="java.lang.String">
            <column name="MashupUserGuid" length="50" not-null="true" />
        </property>
        <property name="lastUpdated" type="java.util.Date">
            <column name="LastUpdated" length="23" not-null="true" />
        </property>
        <property name="fromUrl" type="java.lang.String">
            <column name="FromUrl" length="255" not-null="true" />
        </property>
        <property name="title" type="java.lang.String">
            <column name="Title" length="255" not-null="true" />
        </property>
        <property name="unitName" type="java.lang.String">
            <column name="UnitName" length="255" />
        </property>
        <property name="unitTitle" type="java.lang.String">
            <column name="UnitTitle" length="255" />
        </property>
    </class>
</hibernate-mapping>
