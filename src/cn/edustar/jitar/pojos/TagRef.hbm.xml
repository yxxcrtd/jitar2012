<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.edustar.jitar.pojos.TagRef" table="S_TagRef">
        <id name="id" type="int">
            <column name="Id" />
            <generator class="identity" />
        </id>
        <property name="tagId" type="int">
            <column name="TagId" not-null="true" />
        </property>
        <property name="objectId" type="int">
            <column name="ObjectId" not-null="true" />
        </property>
        <property name="objectType" type="int">
            <column name="ObjectType" not-null="true" />
        </property>
        <property name="orderNum" type="int">
            <column name="OrderNum" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
