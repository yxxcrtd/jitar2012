<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edustar.jitar.pojos.ProductConfig" table="S_ProductConfig">
		<id name="id" type="int">
			<column name="Id" />
			<generator class="identity" />
		</id>
		<property name="ProductID" type="java.lang.String">
			<column name="ProductID" not-null="false" />
		</property>
		<property name="ProductName" type="java.lang.String">
			<column name="ProductName" not-null="false" />
		</property>
		<property name="InstallDate" type="java.lang.String">
			<column name="InstallDate" not-null="false" />
		</property>
	</class>
</hibernate-mapping>
