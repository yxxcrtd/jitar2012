<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.edustar.jitar.pojos.UGroup" table="U_Group">
        <id name="groupId" type="java.lang.Integer">
            <column name="GroupId" />
            <generator class="identity" />
        </id>
        <property name="groupName" type="java.lang.String">
            <column name="GroupName" length="500" />
        </property>
        <property name="groupInfo" type="java.lang.String">
            <column name="GroupInfo" length="5000" />
        </property>
    </class>
</hibernate-mapping>
