<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.edustar.jitar.pojos.SiteTheme" table="S_SiteTheme">
        <id name="siteThemeId" type="java.lang.Integer">
            <column name="SiteThemeId" />
            <generator class="identity" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="Title" length="50" not-null="true" />
        </property>
        <property name="folder" type="java.lang.String">
            <column name="Folder" length="50" not-null="true" />
        </property>
        <property name="status" type="java.lang.Integer">
            <column name="Status" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
