<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.edustar.jitar.pojos.MetaSubject" table="MetaSubject" batch-size="10">
		<id name="msubjId" type="int">
			<column name="MsubjId" />
			<generator class="identity" />
		</id>
		<property name="msubjName" type="java.lang.String">
			<column name="MsubjName" length="32" not-null="true" />
		</property>
		<property name="msubjCode" type="java.lang.String">
			<column name="MsubjCode" length="32" not-null="true" />
		</property>
    <property name="orderNum" type="int">
      <column name="OrderNum" not-null="true" />
    </property>
	</class>
</hibernate-mapping>
