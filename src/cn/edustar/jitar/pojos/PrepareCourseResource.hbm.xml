<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.edustar.jitar.pojos.PrepareCourseResource" table="C_PrepareCourseResource">
        <id name="prepareCourseResourceId" type="java.lang.Integer">
            <column name="PrepareCourseResourceId" />
            <generator class="identity" />
        </id>
        <property name="resourceId" type="java.lang.Integer">
            <column name="ResourceId" not-null="true" />
        </property>
        <property name="prepareCourseStageId" type="java.lang.Integer">
            <column name="PrepareCourseStageId" not-null="true" />
        </property>
        <property name="prepareCourseId" type="java.lang.Integer">
            <column name="PrepareCourseId" not-null="true" />
        </property>
        <property name="userId" type="java.lang.Integer">
            <column name="UserId" not-null="true" />
        </property>
        <property name="createDate" type="java.util.Date">
            <column name="CreateDate" length="23" not-null="true" />
        </property>
        <property name="resourceTitle" type="java.lang.String">
            <column name="ResourceTitle" length="256" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
