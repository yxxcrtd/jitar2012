<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cn.edustar.jitar.pojos.ResourceViewCount" table="ResourceViewCount">
        <composite-id name="id" class="cn.edustar.jitar.pojos.ResourceViewCountId">
            <key-property name="resourceId" type="java.lang.Integer">
                <column name="resourceId" />
            </key-property>
            <key-property name="title" type="java.lang.String">
                <column name="title" length="256" />
            </key-property>
            <key-property name="href" type="java.lang.String">
                <column name="href" length="256" />
            </key-property>
            <key-property name="createDate" type="java.util.Date">
                <column name="createDate" length="23" />
            </key-property>
            <key-property name="fsize" type="java.lang.Integer">
                <column name="fsize" />
            </key-property>
            <key-property name="downloadCount" type="java.lang.Integer">
                <column name="downloadCount" />
            </key-property>
            <key-property name="loginName" type="java.lang.String">
                <column name="loginName" length="50" />
            </key-property>
            <key-property name="nickName" type="java.lang.String">
                <column name="nickName" length="50" />
            </key-property>
            <key-property name="msubjName" type="java.lang.String">
                <column name="msubjName" length="32" />
            </key-property>
            <key-property name="gradeName" type="java.lang.String">
                <column name="gradeName" length="32" />
            </key-property>
            <key-property name="scName" type="java.lang.String">
                <column name="scName" length="64" />
            </key-property>
            <key-property name="viewCount" type="java.lang.Integer">
                <column name="viewCount" />
            </key-property>            
            <key-property name="unitPath" type="java.lang.String">
                <column name="UnitPath" length="512" />
            </key-property>
            <key-property name="sharedDepth" type="int">
                <column name="SharedDepth" />
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
